{
  "manifest_version": 2,
  "name": "日历",
  "version": "1.0",
  "description": "日历插件，备注大小周",
  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "38": "images/icon32.png",
      "48": "images/icon32.png",
      "64": "images/icon32.png",
      "128": "images/icon32.png"
    },
    "default_title": "日历",
    "default_popup": "index.html"
  },
  "background": {
    "page":"background.html"
  },
  "permissions": [
    "storage",
    "http://127.0.0.1:8080/*"
  ]
}